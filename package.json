  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.6.1",
    "chai": "^4.3.4",
    "codecov": "^3.8.3",
    "mocha": "^9.0.3",
