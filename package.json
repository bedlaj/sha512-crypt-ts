    "e2e": "mocha -r ts-node/register tests/*.e2e.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "build": "tsc",
    "docs": "typedoc --out docs ./src"
  },
